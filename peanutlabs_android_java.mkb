#!/usr/bin/env mkb
# Builder mkb file for the java portion of the peanutlabs extension on android
platform JAVA

files
{
    (source/android)
    peanutlabs.java
}

librarypath "$MARMALADE_ROOT/s3e/deploy/plugins/android/android.jar"
librarypath "$MARMALADE_ROOT/s3e/loader/android/s3e_release.jar"
librarypath "source/android/lib/plsdk.jar"

option output-name=lib/android/peanutlabs.jar
